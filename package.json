{
  "name": "definitelytyped-review-bot",
  "version": "0.0.1",
  "description": "DefinitelyTyped review BOT",
  "private": true,
  "scripts": {
    "setup": "dtsm install",
    "build": "tsc -p ./",
    "test": "npm run build",
    "auth": "tsc -p ./ && node utils/authToken.js",
    "start": "node lib/app.js",
    "monitor": "nodemon lib/app.js",
    "deploy": "npm run build && gcloud --project dt-review-bot preview app deploy app.yaml --promote --version prod2 -q --verbosity info",
    "cp-billing": "gsutil cp \"gs://definitelytyped-review-bot/*.json\" ./billing"
  },
  "engines": {
    "node": ">=4.0"
  },
  "license": "MIT",
  "dependencies": {
    "dt-review-tool": "^0.3.0",
    "github": "^0.2.4"
  },
  "devDependencies": {
    "nodemon": "^1.8.1",
    "dtsm": "^0.13.0",
    "typescript": "^1.7.3"
  }
}
